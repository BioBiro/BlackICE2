<Window x:Class="BlackICE2.TestCase"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:BlackICE2"
        mc:Ignorable="d"
        Title="TestCase" Height="300" Width="300">
    <Grid>
        <ComboBox x:Name="cmTestCaseRegisters" HorizontalAlignment="Left" Margin="100,12,0,0" VerticalAlignment="Top" Width="120"/>
        <Label Content="Register" HorizontalAlignment="Left" Margin="45,10,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="tbExpectedResult" HorizontalAlignment="Left" Height="23" Margin="100,215,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="120"/>
        <Label Content="Expected result" HorizontalAlignment="Left" Margin="10,215,0,0" VerticalAlignment="Top"/>
        <Button Content="Add" HorizontalAlignment="Left" Margin="207,243,0,0" VerticalAlignment="Top" Width="75" Click="Button_Click_1"/>

    </Grid>
</Window>
